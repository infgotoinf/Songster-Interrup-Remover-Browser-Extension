{
  "manifest_version": 3,
  "name": "Songsterr Interrupt Remover",
  "version": "1.0",
  "description": "Automatically bypasses Songsterr interruptions",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [{
    "matches": ["*://*.songsterr.com/*"],
    "js": ["content.js"],
    "run_at": "document_end"
  }]
}